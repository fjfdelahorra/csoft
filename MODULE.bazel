module(
    name = "csoft",
    version = "0.1.0",
)

bazel_dep(name = "rules_jvm_external", version = "6.2")

maven = use_extension("@rules_jvm_external//:extensions.bzl", "maven")

maven.install(
    name = "maven",
    artifacts = [
        "io.vertx:vertx-core:4.5.1",
    ],
    repositories = [
        "https://repo.maven.apache.org/maven2",
    ],
)

use_repo(maven, "maven")
